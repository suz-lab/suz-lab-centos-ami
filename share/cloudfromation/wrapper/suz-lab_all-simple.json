                    "KeyName": { "Ref": "KeyName" },
                    "OnDemandStepImageId": { "Ref": "OnDemandStepImageId" },
                    "OnDemandStepInstanceType": { "Ref": "OnDemandStepInstanceType" },
                    "OnDemandStepName": { "Ref": "OnDemandStepName" },
